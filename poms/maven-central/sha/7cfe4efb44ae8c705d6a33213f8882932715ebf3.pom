  <scm>
    <connection>scm:git:git@github.com:Microsoft/Mobius.git</connection>
    <developerConnection>scm:git:git@github.com:Microsoft/Mobius.git</developerConnection>
    <url>git@github.com:Microsoft/Mobius.git</url>
