  <packaging>jar</packaging>
	<url>https://github.com/garyaiki/dendrites</url>
  <organization>
    <name>com.github.garyaiki</name>
  </organization>
	<licenses>
		<license>
			<name>Apache 2.0 License</name>
			<url>http://www.apache.org/licenses/LICENSE-2.0.html</url>
