  <scm>
    <connection>scm:git:git://github.com/beangle/cdi.git</connection>
    <developerConnection>
      scm:git:git@github.com:beangle/cdi.git
    </developerConnection>
    <url>https://github.com/beangle/cdi</url>
    <tag>HEAD</tag>
