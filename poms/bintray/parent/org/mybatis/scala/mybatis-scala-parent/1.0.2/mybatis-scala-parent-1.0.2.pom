<project 
xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.mybatis</groupId>
    <artifactId>mybatis-parent</artifactId>
    <version>20</version>
  </parent>
  <groupId>org.mybatis.scala</groupId>
  <artifactId>mybatis-scala-parent</artifactId>
  <version>1.0.2</version>
  <packaging>pom</packaging>
  <name>MyBatis Scala</name>
  <url>http://www.mybatis.org/scala</url>
  <modules>
    <module>mybatis-scala-core</module>
    <module>mybatis-scala-samples</module>
  </modules>
  <scm>
    <url>http://github.com/mybatis/scala</url>
    <connection>scm:git:ssh://github.com/mybatis/scala.git</connection>
    <developerConnection>
      scm:git:git+ssh://git@github.com/mybatis/scala.git
    </developerConnection>
    <tag>mybatis-scala-parent-1.0.2</tag>
  </scm>
  <issueManagement>
    <system>GitHub Issue Management</system>
    <url>https://github.com/mybatis/scala/issues</url>
  </issueManagement>
  <ciManagement>
    <system>Travis CI</system>
    <url>https://travis-ci.org/mybatis/scala</url>
  </ciManagement>
  <distributionManagement>
    <site>
      <id>github</id>
      <url>gitsite:git@github.com/mybatis/scala.git</url>
    </site>
  </distributionManagement>
  <properties>
    <findbugs.onlyAnalyze>org.mybatis.scala.*</findbugs.onlyAnalyze>
    <javac.src.version>1.6</javac.src.version>
    <javac.target.version>1.6</javac.target.version>
    <gcu.product>Scala</gcu.product>
  </properties>
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-release-plugin</artifactId>
        <configuration>
          <arguments>-Prelease,bundle,gupload</arguments>
        </configuration>
      </plugin>
    </plugins>
    <pluginManagement>
      <plugins>
        <plugin>
          <groupId>org.scala-tools</groupId>
          <artifactId>maven-scala-plugin</artifactId>
          <version>2.15.2</version>
          <configuration>
            <args>
              <arg>-deprecation</arg>
            </args>
          </configuration>
          <executions>
            <execution>
              <id>compile</id>
              <goals>
                <goal>compile</goal>
              </goals>
              <phase>compile</phase>
            </execution>
            <execution>
              <id>test-compile</id>
              <goals>
                <goal>testCompile</goal>
              </goals>
              <phase>test-compile</phase>
            </execution>
          </executions>
        </plugin>
      </plugins>
    </pluginManagement>
  </build>
</project>

